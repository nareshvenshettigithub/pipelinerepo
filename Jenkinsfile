pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                echo "hello world this is my first declarative script for built"
            }
        }
          stage('test'){
            steps{
                echo "hello world this is my first declarative script for test"
            }
        }
          stage('deploy'){
            steps{
                echo "hello world this is my first declarative script for deploy"
            }
        }
          stage('delivery'){
            steps{
                echo "hello world this is my first declarative script for delivery"
            }
        }
    }
}
